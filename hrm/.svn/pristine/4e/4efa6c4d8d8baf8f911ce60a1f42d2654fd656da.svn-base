package com.minori.hrm.models;

import java.io.Serializable;
import java.sql.Timestamp;
import java.util.List;
import javax.persistence.Basic;
import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.FetchType;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.Lob;
import javax.persistence.ManyToOne;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.OneToMany;
import javax.persistence.Table;
import javax.persistence.Temporal;
import javax.persistence.TemporalType;

/**
 *
 * @author Admin
 */
@Entity
@Table(name = "rcm_candidate")
@NamedQueries({
    @NamedQuery(name = "RcmCandidate.findAll", query = "SELECT r FROM RcmCandidate r")})
public class RcmCandidate implements Serializable {

    private static final long serialVersionUID = 1L;
    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    
    @Column(name = "candidate_id")
    private Integer candidateId;
    @Lob
    @Column(name = "description")
    private String description;
    
    @Column(name = "name")
    private String name;
    @Column(name = "phone")
    private String phone;
    @Column(name = "link_cv")
    private String linkCv;
    @Column(name = "step")
    private Integer step;
    
    @Column(name = "status")
    private String status;
    @Column(name = "email")
    private String email;
    
    @Column(name = "priority")
    private int priority;
    @Column(name = "created_date")
    
    private Timestamp createdDate;
    @Column(name = "updated_date")
    
    private Timestamp updatedDate;
    
    @Column(name = "created_by")
    private String createdBy;
    @Column(name = "updated_by")
    private String updatedBy;
    @JoinColumn(name = "recruitment_id", referencedColumnName = "recruitment_id")
    @ManyToOne(optional = false, fetch = FetchType.LAZY)
    private RcmRecruitmentPlan rcmRecruitmentPlan;
    @OneToMany(mappedBy = "rcmCandidate", fetch = FetchType.LAZY)
    private List<RcmCandidateCalendar> rcmCandidateCalendarList;

    public RcmCandidate() {
    }

    public RcmCandidate(Integer candidateId) {
        this.candidateId = candidateId;
    }

    public RcmCandidate(Integer candidateId, String name, String status, int priority, String createdBy) {
        this.candidateId = candidateId;
        this.name = name;
        this.status = status;
        this.priority = priority;
        this.createdBy = createdBy;
    }

    public Integer getCandidateId() {
        return candidateId;
    }

    public void setCandidateId(Integer candidateId) {
        this.candidateId = candidateId;
    }

    public String getDescription() {
        return description;
    }

    public void setDescription(String description) {
        this.description = description;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getPhone() {
        return phone;
    }

    public void setPhone(String phone) {
        this.phone = phone;
    }

    public String getLinkCv() {
        return linkCv;
    }

    public void setLinkCv(String linkCv) {
        this.linkCv = linkCv;
    }

    public Integer getStep() {
        return step;
    }

    public void setStep(Integer step) {
        this.step = step;
    }

    public String getStatus() {
        return status;
    }

    public void setStatus(String status) {
        this.status = status;
    }

    public String getEmail() {
        return email;
    }

    public void setEmail(String email) {
        this.email = email;
    }

    public int getPriority() {
        return priority;
    }

    public void setPriority(int priority) {
        this.priority = priority;
    }

    public Timestamp getCreatedDate() {
        return createdDate;
    }

    public void setCreatedDate(Timestamp createdDate) {
        this.createdDate = createdDate;
    }

    public Timestamp getUpdatedDate() {
        return updatedDate;
    }

    public void setUpdatedDate(Timestamp updatedDate) {
        this.updatedDate = updatedDate;
    }

    public String getCreatedBy() {
        return createdBy;
    }

    public void setCreatedBy(String createdBy) {
        this.createdBy = createdBy;
    }

    public String getUpdatedBy() {
        return updatedBy;
    }

    public void setUpdatedBy(String updatedBy) {
        this.updatedBy = updatedBy;
    }

    public RcmRecruitmentPlan getRcmRecruitmentPlan() {
        return rcmRecruitmentPlan;
    }

    public void setRcmRecruitmentPlan(RcmRecruitmentPlan rcmRecruitmentPlan) {
        this.rcmRecruitmentPlan = rcmRecruitmentPlan;
    }

    public List<RcmCandidateCalendar> getRcmCandidateCalendarList() {
        return rcmCandidateCalendarList;
    }

    public void setRcmCandidateCalendarList(List<RcmCandidateCalendar> rcmCandidateCalendarList) {
        this.rcmCandidateCalendarList = rcmCandidateCalendarList;
    }

    @Override
    public int hashCode() {
        int hash = 0;
        hash += (candidateId != null ? candidateId.hashCode() : 0);
        return hash;
    }

    @Override
    public boolean equals(Object object) {
        // TODO: Warning - this method won't work in the case the id fields are not set
        if (!(object instanceof RcmCandidate)) {
            return false;
        }
        RcmCandidate other = (RcmCandidate) object;
        if ((this.candidateId == null && other.candidateId != null) || (this.candidateId != null && !this.candidateId.equals(other.candidateId))) {
            return false;
        }
        return true;
    }

    @Override
    public String toString() {
        return "hrm.RcmCandidate[ candidateId=" + candidateId + " ]";
    }
    
}
