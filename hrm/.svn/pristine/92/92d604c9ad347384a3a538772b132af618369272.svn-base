package com.minori.hrm.dto;

/**
 * 
 * @author KhanhNM
 *
 */
public class MasterLeaveReasonDto {

	private int leaveReasonId;
	
	private String description; 

	private int leaveType;

	private String name;
	
	private String totalOffDay;

	/**
	 * @return the leaveReasonId
	 */
	public int getLeaveReasonId() {
		return leaveReasonId;
	}

	/**
	 * @return the description
	 */
	public String getDescription() {
		return description;
	}

	/**
	 * @return the leaveType
	 */
	public int getLeaveType() {
		return leaveType;
	}

	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}

	/**
	 * @return the totalOffDay
	 */
	public String getTotalOffDay() {
		return totalOffDay;
	}

	/**
	 * @param leaveReasonId the leaveReasonId to set
	 */
	public void setLeaveReasonId(int leaveReasonId) {
		this.leaveReasonId = leaveReasonId;
	}

	/**
	 * @param description the description to set
	 */
	public void setDescription(String description) {
		this.description = description;
	}

	/**
	 * @param leaveType the leaveType to set
	 */
	public void setLeaveType(int leaveType) {
		this.leaveType = leaveType;
	}

	/**
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}

	/**
	 * @param totalOffDay the totalOffDay to set
	 */
	public void setTotalOffDay(String totalOffDay) {
		this.totalOffDay = totalOffDay;
	}

}
