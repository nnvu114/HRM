package com.minori.hrm.models;

import java.io.Serializable;
import javax.persistence.Basic;
import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Lob;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

/**
 *
 * @author Admin
 */
@Entity
@Table(name = "master_allowance")
@NamedQueries({
    @NamedQuery(name = "MasterAllowance.findAll", query = "SELECT m FROM MasterAllowance m")})
public class MasterAllowance implements Serializable {

    private static final long serialVersionUID = 1L;
    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    
    @Column(name = "allowance_master_id")
    private Integer allowanceMasterId;
    
    @Column(name = "allowance_name")
    private String allowanceName;
    
    @Lob
    @Column(name = "allowance_des")
    private String allowanceDes;
    
    @Column(name = "amount")
    private double amount;

    public MasterAllowance() {
    }

    public MasterAllowance(Integer allowanceMasterId) {
        this.allowanceMasterId = allowanceMasterId;
    }

    public MasterAllowance(Integer allowanceMasterId, String allowanceName, String allowanceDes, double amount) {
        this.allowanceMasterId = allowanceMasterId;
        this.allowanceName = allowanceName;
        this.allowanceDes = allowanceDes;
        this.amount = amount;
    }

    public Integer getAllowanceMasterId() {
        return allowanceMasterId;
    }

    public void setAllowanceMasterId(Integer allowanceMasterId) {
        this.allowanceMasterId = allowanceMasterId;
    }

    public String getAllowanceName() {
        return allowanceName;
    }

    public void setAllowanceName(String allowanceName) {
        this.allowanceName = allowanceName;
    }

    public String getAllowanceDes() {
        return allowanceDes;
    }

    public void setAllowanceDes(String allowanceDes) {
        this.allowanceDes = allowanceDes;
    }

    public double getAmount() {
        return amount;
    }

    public void setAmount(double amount) {
        this.amount = amount;
    }

    @Override
    public int hashCode() {
        int hash = 0;
        hash += (allowanceMasterId != null ? allowanceMasterId.hashCode() : 0);
        return hash;
    }

    @Override
    public boolean equals(Object object) {
        // TODO: Warning - this method won't work in the case the id fields are not set
        if (!(object instanceof MasterAllowance)) {
            return false;
        }
        MasterAllowance other = (MasterAllowance) object;
        if ((this.allowanceMasterId == null && other.allowanceMasterId != null) || (this.allowanceMasterId != null && !this.allowanceMasterId.equals(other.allowanceMasterId))) {
            return false;
        }
        return true;
    }

    @Override
    public String toString() {
        return "hrm.MasterAllowance[ allowanceMasterId=" + allowanceMasterId + " ]";
    }
    
}
