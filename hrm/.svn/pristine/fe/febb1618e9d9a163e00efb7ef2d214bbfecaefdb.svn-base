package com.minori.hrm.models;

import java.io.Serializable;
import javax.persistence.Basic;
import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Lob;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

/**
 *
 * @author Admin
 */
@Entity
@Table(name = "master_evaluation")
@NamedQueries({
    @NamedQuery(name = "MasterEvaluation.findAll", query = "SELECT m FROM MasterEvaluation m")})
public class MasterEvaluation implements Serializable {

    private static final long serialVersionUID = 1L;
    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    
    @Column(name = "evaluation_master_id")
    private Integer evaluationMasterId;
    
    @Column(name = "evaluation_title")
    private String evaluationTitle;
    
    @Lob
    @Column(name = "evaluation_des")
    private String evaluationDes;

    public MasterEvaluation() {
    }

    public MasterEvaluation(Integer evaluationMasterId) {
        this.evaluationMasterId = evaluationMasterId;
    }

    public MasterEvaluation(Integer evaluationMasterId, String evaluationTitle, String evaluationDes) {
        this.evaluationMasterId = evaluationMasterId;
        this.evaluationTitle = evaluationTitle;
        this.evaluationDes = evaluationDes;
    }

    public Integer getEvaluationMasterId() {
        return evaluationMasterId;
    }

    public void setEvaluationMasterId(Integer evaluationMasterId) {
        this.evaluationMasterId = evaluationMasterId;
    }

    public String getEvaluationTitle() {
        return evaluationTitle;
    }

    public void setEvaluationTitle(String evaluationTitle) {
        this.evaluationTitle = evaluationTitle;
    }

    public String getEvaluationDes() {
        return evaluationDes;
    }

    public void setEvaluationDes(String evaluationDes) {
        this.evaluationDes = evaluationDes;
    }

    @Override
    public int hashCode() {
        int hash = 0;
        hash += (evaluationMasterId != null ? evaluationMasterId.hashCode() : 0);
        return hash;
    }

    @Override
    public boolean equals(Object object) {
        // TODO: Warning - this method won't work in the case the id fields are not set
        if (!(object instanceof MasterEvaluation)) {
            return false;
        }
        MasterEvaluation other = (MasterEvaluation) object;
        if ((this.evaluationMasterId == null && other.evaluationMasterId != null) || (this.evaluationMasterId != null && !this.evaluationMasterId.equals(other.evaluationMasterId))) {
            return false;
        }
        return true;
    }

    @Override
    public String toString() {
        return "hrm.MasterEvaluation[ evaluationMasterId=" + evaluationMasterId + " ]";
    }
    
}
