package com.minori.hrm.models;

import java.io.Serializable;
import java.sql.Timestamp;
import javax.persistence.Basic;
import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.FetchType;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.Lob;
import javax.persistence.ManyToOne;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
import javax.persistence.Temporal;
import javax.persistence.TemporalType;

/**
 *
 * @author Admin
 */
@Entity
@Table(name = "epm_contract")
@NamedQueries({
    @NamedQuery(name = "EpmContract.findAll", query = "SELECT e FROM EpmContract e")})
public class EpmContract implements Serializable {

    private static final long serialVersionUID = 1L;
    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    
    @Column(name = "contractr_id")
    private Integer contractrId;
    
    @Column(name = "contract_master_id")
    private short contractMasterId;
    @Column(name = "contract_no")
    private String contractNo;
    
    @Column(name = "position_id")
    private String positionId;
    
    @Lob
    @Column(name = "salary")
    private String salary;
    
    @Column(name = "start_date")
    
    private Timestamp startDate;
    
    @Column(name = "expired_date")
    
    private Timestamp expiredDate;
    
    @Column(name = "status")
    private int status;
    @Column(name = "sign_date")
    
    private Timestamp signDate;
    @Column(name = "attachment")
    private String attachment;
    
    @Column(name = "delete")
    private boolean delete;
    @Column(name = "approve_by")
    private String approveBy;
    @Column(name = "created_date")
    
    private Timestamp createdDate;
    @Column(name = "updated_date")
    
    private Timestamp updatedDate;
    
    @Column(name = "created_by")
    private String createdBy;
    @Column(name = "updated_by")
    private String updatedBy;
    @JoinColumn(name = "employee_id", referencedColumnName = "employee_id")
    @ManyToOne(optional = false, fetch = FetchType.LAZY)
    private Employee employee;

    public EpmContract() {
    }

    public EpmContract(Integer contractrId) {
        this.contractrId = contractrId;
    }

    public EpmContract(Integer contractrId, short contractMasterId, String positionId, String salary, Timestamp startDate, Timestamp expiredDate, int status, boolean delete, String createdBy) {
        this.contractrId = contractrId;
        this.contractMasterId = contractMasterId;
        this.positionId = positionId;
        this.salary = salary;
        this.startDate = startDate;
        this.expiredDate = expiredDate;
        this.status = status;
        this.delete = delete;
        this.createdBy = createdBy;
    }

    public Integer getContractrId() {
        return contractrId;
    }

    public void setContractrId(Integer contractrId) {
        this.contractrId = contractrId;
    }

    public short getContractMasterId() {
        return contractMasterId;
    }

    public void setContractMasterId(short contractMasterId) {
        this.contractMasterId = contractMasterId;
    }

    public String getContractNo() {
        return contractNo;
    }

    public void setContractNo(String contractNo) {
        this.contractNo = contractNo;
    }

    public String getPositionId() {
        return positionId;
    }

    public void setPositionId(String positionId) {
        this.positionId = positionId;
    }

    public String getSalary() {
        return salary;
    }

    public void setSalary(String salary) {
        this.salary = salary;
    }

    public Timestamp getStartDate() {
        return startDate;
    }

    public void setStartDate(Timestamp startDate) {
        this.startDate = startDate;
    }

    public Timestamp getExpiredDate() {
        return expiredDate;
    }

    public void setExpiredDate(Timestamp expiredDate) {
        this.expiredDate = expiredDate;
    }

    public int getStatus() {
        return status;
    }

    public void setStatus(int status) {
        this.status = status;
    }

    public Timestamp getSignDate() {
        return signDate;
    }

    public void setSignDate(Timestamp signDate) {
        this.signDate = signDate;
    }

    public String getAttachment() {
        return attachment;
    }

    public void setAttachment(String attachment) {
        this.attachment = attachment;
    }

    public boolean getDelete() {
        return delete;
    }

    public void setDelete(boolean delete) {
        this.delete = delete;
    }

    public String getApproveBy() {
        return approveBy;
    }

    public void setApproveBy(String approveBy) {
        this.approveBy = approveBy;
    }

    public Timestamp getCreatedDate() {
        return createdDate;
    }

    public void setCreatedDate(Timestamp createdDate) {
        this.createdDate = createdDate;
    }

    public Timestamp getUpdatedDate() {
        return updatedDate;
    }

    public void setUpdatedDate(Timestamp updatedDate) {
        this.updatedDate = updatedDate;
    }

    public String getCreatedBy() {
        return createdBy;
    }

    public void setCreatedBy(String createdBy) {
        this.createdBy = createdBy;
    }

    public String getUpdatedBy() {
        return updatedBy;
    }

    public void setUpdatedBy(String updatedBy) {
        this.updatedBy = updatedBy;
    }

    public Employee getEmployee() {
        return employee;
    }

    public void setEmployee(Employee employee) {
        this.employee = employee;
    }

    @Override
    public int hashCode() {
        int hash = 0;
        hash += (contractrId != null ? contractrId.hashCode() : 0);
        return hash;
    }

    @Override
    public boolean equals(Object object) {
        // TODO: Warning - this method won't work in the case the id fields are not set
        if (!(object instanceof EpmContract)) {
            return false;
        }
        EpmContract other = (EpmContract) object;
        if ((this.contractrId == null && other.contractrId != null) || (this.contractrId != null && !this.contractrId.equals(other.contractrId))) {
            return false;
        }
        return true;
    }

    @Override
    public String toString() {
        return "hrm.EpmContract[ contractrId=" + contractrId + " ]";
    }
    
}
