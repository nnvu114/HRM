    public void setEvlKpiResultList(List<EvlKpiResult> evlKpiResultList) {
        this.evlKpiResultList = evlKpiResultList;
    }

    public List<EvlFeedbackKpi> getEvlFeedbackKpiList() {
        return evlFeedbackKpiList;
    }

    public void setEvlFeedbackKpiList(List<EvlFeedbackKpi> evlFeedbackKpiList) {
        this.evlFeedbackKpiList = evlFeedbackKpiList;
    }

    public List<EvlKpiCriteria> getEvlKpiCriteriaList() {
        return evlKpiCriteriaList;
    }

    public void setEvlKpiCriteriaList(List<EvlKpiCriteria> evlKpiCriteriaList) {
        this.evlKpiCriteriaList = evlKpiCriteriaList;
    }

    @Override
    public int hashCode() {
        int hash = 0;
        hash += (kpiId != null ? kpiId.hashCode() : 0);
        return hash;
    }

    @Override
    public boolean equals(Object object) {
        // TODO: Warning - this method won't work in the case the id fields are not set
        if (!(object instanceof EvlKpi)) {
            return false;
        }
        EvlKpi other = (EvlKpi) object;
        if ((this.kpiId == null && other.kpiId != null) || (this.kpiId != null && !this.kpiId.equals(other.kpiId))) {
