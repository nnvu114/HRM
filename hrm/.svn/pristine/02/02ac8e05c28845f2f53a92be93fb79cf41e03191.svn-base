    @OneToMany(cascade = CascadeType.ALL, mappedBy = "evlKpi", fetch = FetchType.LAZY)
    private List<EvlKpiResult> evlKpiResultList;
    @OneToMany(cascade = CascadeType.ALL, mappedBy = "evlKpi", fetch = FetchType.LAZY)
