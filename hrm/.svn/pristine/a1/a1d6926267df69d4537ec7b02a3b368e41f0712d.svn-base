package com.minori.hrm.models;

import java.io.Serializable;
import javax.persistence.Basic;
import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Lob;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

/**
 *
 * @author Admin
 */
@Entity
@Table(name = "master_relationship")
@NamedQueries({
    @NamedQuery(name = "MasterRelationship.findAll", query = "SELECT m FROM MasterRelationship m")})
public class MasterRelationship implements Serializable {

    private static final long serialVersionUID = 1L;
    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    
    @Column(name = "relationship_master_id")
    private Integer relationshipMasterId;
    @Column(name = "relationship_name")
    private String relationshipName;
    @Lob
    @Column(name = "relationship_des")
    private String relationshipDes;

    public MasterRelationship() {
    }

    public MasterRelationship(Integer relationshipMasterId) {
        this.relationshipMasterId = relationshipMasterId;
    }

    public Integer getRelationshipMasterId() {
        return relationshipMasterId;
    }

    public void setRelationshipMasterId(Integer relationshipMasterId) {
        this.relationshipMasterId = relationshipMasterId;
    }

    public String getRelationshipName() {
        return relationshipName;
    }

    public void setRelationshipName(String relationshipName) {
        this.relationshipName = relationshipName;
    }

    public String getRelationshipDes() {
        return relationshipDes;
    }

    public void setRelationshipDes(String relationshipDes) {
        this.relationshipDes = relationshipDes;
    }

    @Override
    public int hashCode() {
        int hash = 0;
        hash += (relationshipMasterId != null ? relationshipMasterId.hashCode() : 0);
        return hash;
    }

    @Override
    public boolean equals(Object object) {
        // TODO: Warning - this method won't work in the case the id fields are not set
        if (!(object instanceof MasterRelationship)) {
            return false;
        }
        MasterRelationship other = (MasterRelationship) object;
        if ((this.relationshipMasterId == null && other.relationshipMasterId != null) || (this.relationshipMasterId != null && !this.relationshipMasterId.equals(other.relationshipMasterId))) {
            return false;
        }
        return true;
    }

    @Override
    public String toString() {
        return "hrm.MasterRelationship[ relationshipMasterId=" + relationshipMasterId + " ]";
    }
    
}
