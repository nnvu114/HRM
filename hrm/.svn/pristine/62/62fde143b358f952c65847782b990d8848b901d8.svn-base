package com.minori.hrm.models;

import java.io.Serializable;
import java.sql.Timestamp;
import javax.persistence.Basic;
import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.FetchType;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.Lob;
import javax.persistence.ManyToOne;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
import javax.persistence.Temporal;
import javax.persistence.TemporalType;

/**
 *
 * @author Admin
 */
@Entity
@Table(name = "tnm_inform_training")
@NamedQueries({
    @NamedQuery(name = "TnmInformTraining.findAll", query = "SELECT t FROM TnmInformTraining t")})
public class TnmInformTraining implements Serializable {

    private static final long serialVersionUID = 1L;
    @Id
    
    @Column(name = "inform_training_id")
    private Integer informTrainingId;
    @Column(name = "result")
    private String result;
    
    @Column(name = "status")
    private int status;
    @Lob
    @Column(name = "evaluation_of_trainer")
    private String evaluationOfTrainer;
    @Lob
    @Column(name = "evaluation_of_employee")
    private String evaluationOfEmployee;
    @Column(name = "created_date")
    
    private Timestamp createdDate;
    @Column(name = "updated_date")
    
    private Timestamp updatedDate;
    
    @Column(name = "created_by")
    private String createdBy;
    @Column(name = "updated_by")
    private String updatedBy;
    @JoinColumn(name = "employee_id", referencedColumnName = "employee_id")
    @ManyToOne(fetch = FetchType.LAZY)
    private Employee employee;
    @JoinColumn(name = "training_id", referencedColumnName = "training_id")
    @ManyToOne(fetch = FetchType.LAZY)
    private TnmTraining tnmTraining;

    public TnmInformTraining() {
    }

    public TnmInformTraining(Integer informTrainingId) {
        this.informTrainingId = informTrainingId;
    }

    public TnmInformTraining(Integer informTrainingId, int status, String createdBy) {
        this.informTrainingId = informTrainingId;
        this.status = status;
        this.createdBy = createdBy;
    }

    public Integer getInformTrainingId() {
        return informTrainingId;
    }

    public void setInformTrainingId(Integer informTrainingId) {
        this.informTrainingId = informTrainingId;
    }

    public String getResult() {
        return result;
    }

    public void setResult(String result) {
        this.result = result;
    }

    public int getStatus() {
        return status;
    }

    public void setStatus(int status) {
        this.status = status;
    }

    public String getEvaluationOfTrainer() {
        return evaluationOfTrainer;
    }

    public void setEvaluationOfTrainer(String evaluationOfTrainer) {
        this.evaluationOfTrainer = evaluationOfTrainer;
    }

    public String getEvaluationOfEmployee() {
        return evaluationOfEmployee;
    }

    public void setEvaluationOfEmployee(String evaluationOfEmployee) {
        this.evaluationOfEmployee = evaluationOfEmployee;
    }

    public Timestamp getCreatedDate() {
        return createdDate;
    }

    public void setCreatedDate(Timestamp createdDate) {
        this.createdDate = createdDate;
    }

    public Timestamp getUpdatedDate() {
        return updatedDate;
    }

    public void setUpdatedDate(Timestamp updatedDate) {
        this.updatedDate = updatedDate;
    }

    public String getCreatedBy() {
        return createdBy;
    }

    public void setCreatedBy(String createdBy) {
        this.createdBy = createdBy;
    }

    public String getUpdatedBy() {
        return updatedBy;
    }

    public void setUpdatedBy(String updatedBy) {
        this.updatedBy = updatedBy;
    }

    public Employee getEmployee() {
        return employee;
    }

    public void setEmployee(Employee employee) {
        this.employee = employee;
    }

    public TnmTraining getTnmTraining() {
        return tnmTraining;
    }

    public void setTnmTraining(TnmTraining tnmTraining) {
        this.tnmTraining = tnmTraining;
    }

    @Override
    public int hashCode() {
        int hash = 0;
        hash += (informTrainingId != null ? informTrainingId.hashCode() : 0);
        return hash;
    }

    @Override
    public boolean equals(Object object) {
        // TODO: Warning - this method won't work in the case the id fields are not set
        if (!(object instanceof TnmInformTraining)) {
            return false;
        }
        TnmInformTraining other = (TnmInformTraining) object;
        if ((this.informTrainingId == null && other.informTrainingId != null) || (this.informTrainingId != null && !this.informTrainingId.equals(other.informTrainingId))) {
            return false;
        }
        return true;
    }

    @Override
    public String toString() {
        return "hrm.TnmInformTraining[ informTrainingId=" + informTrainingId + " ]";
    }
    
}
